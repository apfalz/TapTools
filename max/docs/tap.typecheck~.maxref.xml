<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>

<c74object name="tap.typecheck~" module="msp" category="TapTools, Control, Patching">

	<digest>
		Sort information by data type
	</digest>

	<description>
		The <o>tap.typecheck~</o> object 
		sorts the input based on data-type. This is very handy when building sub-patches or abstractions.
	</description>

	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">74 Objects</metadata>
		<metadata name="tag">TapTools</metadata>
		<metadata name="tag">Audio</metadata>
		<metadata name="tag">Analysis</metadata>
		<metadata name="tag">Routing</metadata>
	</metadatalist>


	<!--MESSAGES-->
	<methodlist>
		
		<method name="anything">
			<arglist><arg name="input" type="list" optional="0" id="0" /></arglist>
			<description><o>tap.typecheck~</o> takes any data-type and routes it to an outlet accordingly. </description>
		</method>
		
	</methodlist>
	
	
	<!--OUTPUT-->
	<misc name="Output">
		<entry name="signal">
			<description>Type-checked as a signal.</description>
		</entry>
		<entry name="bang">
			<description>Type-checked as a bang.</description>
		</entry>
		<entry name="int">
			<description>Type-checked as an integer.</description>
		</entry>
		<entry name="float">
			<description>Type-checked as a float.</description>
		</entry>
		<entry name="list">
			<description>Type-checked as a list beginning with a symbol.</description>
		</entry>
		<entry name="symbol">
			<description>Type-checked as a symbol.</description>
		</entry>
		<entry name="list">
			<description>Type-checked as a legal-list beginning with a number.</description>
		</entry>
	</misc>
	
	
	<!--SEEALSO-->
	<seealsolist>
		<seealso name="tap.typecheck"/>
		<seealso name="tap.route"/>
		<seealso name="pack"/>
		<seealso name="tap.pack"/>
		<seealso name="unpack"/>
	</seealsolist>

	
</c74object>
