<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
 
<c74object name="tap.rms~" module="msp" category="TapTools, MSP Analysis">

	<digest>
		Root-mean-square of a signal
	</digest>

	<description>
		The <o>tap.rms~</o> object 
		outputs the power of an input signal.
		This is useful for a number of analysis and envelope-following applications.
	</description>

	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">74 Objects</metadata>
		<metadata name="tag">TapTools</metadata>
		<metadata name="tag">Audio</metadata>
		<metadata name="tag">Analysis</metadata>
		<metadata name="tag">Math</metadata>
		<metadata name="tag">Envelope</metadata>
	</metadatalist>

	
	<!--MESSAGES-->
	<methodlist>	
		<method name="float">
			<arglist>
				<arg name="input" type="float" optional="0" id="0" />
			</arglist>
			<description>Sets the feedback coefficient.</description>
		</method>
	</methodlist>
	
	
	<!--ATTRIBUTES-->
	<attributelist>
		<attribute name="feedback_coefficient" get="1" set="1" type="int" size="1">
			<description>Sets the feedback coefficient</description>
		</attribute>
	</attributelist>
	
	
	<!--SEEALSO-->
	<seealsolist>
		<seealso name="average~"/>
	</seealsolist>

	
</c74object>
